  "license": "MIT",
  "repository": "jongleberry/eslint-config-jongleberry",
  "dependencies": {
    "babel-eslint": "^6.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^2.0.0",
    "eslint-config-standard-react": "^4.0.2",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^2.0.0",
