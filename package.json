{
  "name": "eslint-config-jongleberry",
  "description": "My personal eslint config",
  "version": "1.2.2",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jongleberry/eslint-config-jongleberry",
  "dependencies": {
    "babel-eslint": "5.0.0-beta6",
    "eslint-config-airbnb": "3.1.0",
    "eslint-config-eslint": "1.0.1",
    "eslint-config-google": "0.3.0",
    "eslint-config-standard": "4.4.0",
    "eslint-config-standard-react": "1.2.1",
    "eslint-plugin-react": "3.15.0",
    "eslint-plugin-standard": "1.3.1"
  },
  "devDependencies": {
    "codecov": "1.0.1",
    "eslint": "1.10.3",
    "ghooks": "1.0.3",
    "mocha": "2.3.4",
    "nyc": "5.3.0"
  },
  "scripts": {
    "lint": "eslint --cache lib test",
    "test": "mocha",
    "test-cov": "nyc node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "eslint",
    "config"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
