{
  "name": "eslint-config-jongleberry",
  "description": "My personal eslint config",
  "version": "2.1.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jongleberry/eslint-config-jongleberry",
  "dependencies": {
    "babel-eslint": "^6.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.2"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "^2.5.3",
    "ghooks": "^1.0.3",
    "mocha": "^2.4.5",
    "nyc": "^6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache index.js react.js test",
    "test": "mocha",
    "test-cov": "nyc node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "eslint",
    "config"
  ],
  "files": [
    "index.js",
    "react.js"
  ],
  "main": "index.js"
}
